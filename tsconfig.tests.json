{
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "declaration": true,
    "target": "ESNext",
  },
  "include": ["tests/**/*","vitest.config.ts"],
  "exclude": [
    "node_modules",
  ],
}
